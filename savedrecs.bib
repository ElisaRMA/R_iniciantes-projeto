@article{ ISI:000419662800001,
Author = {Saito, Kazuki},
Title = {{Development of Plant Metabolomics and Medicinal Plant Genomics}},
Journal = {{YAKUGAKU ZASSHI-JOURNAL OF THE PHARMACEUTICAL SOCIETY OF JAPAN}},
