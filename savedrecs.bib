@article{ ISI:000322019100015,
Author = {Gaquerel, Emmanuel and Kuhl, Carsten and Neumann, Steffen},
Title = {{Computational annotation of plant metabolomics profiles via a novel
   network-assisted approach}},
Journal = {{METABOLOMICS}},
