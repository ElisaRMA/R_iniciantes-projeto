@article{ ISI:000276335900005,
Author = {Bais, Preeti and Moon, Stephanie M. and He, Kun and Leitao, Ricardo and
   Dreher, Kate and Walk, Tom and Sucaet, Yves and Barkan, Lenore and
   Wohlgemuth, Gert and Roth, Mary R. and Wurtele, Eve Syrkin and Dixon,
   Philip and Fiehn, Oliver and Lange, B. Markus and Shulaev, Vladimir and
   Sumner, Lloyd W. and Welti, Ruth and Nikolau, Basil J. and Rhee, Seung
   Y. and Dickerson, Julie A.},
Title = {{PlantMetabolomics.org: A Web Portal for Plant Metabolomics Experiments}},
